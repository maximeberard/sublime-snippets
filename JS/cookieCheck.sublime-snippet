if(document.cookie.match(new RegExp('${1:cookieName}' + '=([^;]+)')) === null){

    var expiryDate = new Date();
    expiryDate.setFullYear(expiryDate.getFullYear() + 1);
    document.cookie = '$1=y; expires=' + expiryDate.toGMTString();
}
